        <span class="n">Test_queue</span><span class="o">&lt;</span><span class="nc">Integer</span><span class="o">&gt;</span> <span class="n">tq</span> <span class="o">=</span> <span class="k">new</span> <span class="n">Test_queue</span><span class="o">&lt;</span><span class="nc">Integer</span><span class="o">&gt;();</span>
        <span class="n">tq</span><span class="o">.</span><span class="na">enqueue</span><span class="o">(</span><span class="mi">1</span><span class="o">);</span>  <span class="c1">//1ì„ ë„£ê³ </span>
        <span class="n">tq</span><span class="o">.</span><span class="na">enqueue</span><span class="o">(</span><span class="mi">2</span><span class="o">);</span>  <span class="c1">//2ë¥¼ ë„£ê³ </span>
        <span class="n">tq</span><span class="o">.</span><span class="na">enqueue</span><span class="o">(</span><span class="mi">3</span><span class="o">);</span>  <span class="c1">//3ì„ ë„£ì—ˆë‹¤</span>
        <span class="nc">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">println</span><span class="o">(</span><span class="n">tq</span><span class="o">.</span><span class="na">dequeue</span><span class="o">());</span> <span class="c1">//1ì„ ë¹¼ê³ </span>
